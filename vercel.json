{
  "builds": [
    {
      "src": "config/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "config/wsgi.py"
    }
  ],
  "buildCommand": "pip install --use-pep517 -r requirements.txt && python manage.py collectstatic --noinput",
  "env": {
    "DJANGO_SETTINGS_MODULE": "config.settings.development"
  }
}
