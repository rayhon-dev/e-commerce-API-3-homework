{
  "builds": [
    {
      "src": "config/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "config/wsgi.py"
    }
  ],
  "buildCommand": "pip install --use-pep517 -r requirements.txt",
  "env": {
    "DJANGO_SETTINGS_MODULE": "config.production"
  }
}
